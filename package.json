{
  "name": "spell-platform",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings 10",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:seed": "tsx prisma/seed.ts",
    "test": "node --import tsx --test \"tests/lib/*.test.ts\"",
    "spell": "tsx cli/spell.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.927.0",
    "@prisma/client": "^6.18.0",
    "@simplewebauthn/server": "9",
    "@simplewebauthn/types": "9",
    "@upstash/redis": "^1.35.6",
    "dotenv": "^17.2.3",
    "nats": "^2.21.0",
    "next": "16.0.0",
    "next-auth": "5.0.0-beta.29",
    "prisma": "^6.18.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "latest",
    "stripe": "latest",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.46.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.1"
}
